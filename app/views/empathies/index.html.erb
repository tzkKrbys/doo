<p id="notice"><%= notice %></p>

<h1>Listing Empathies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empathies.each do |empathy| %>
      <tr>
        <td><%= link_to 'Show', empathy %></td>
        <td><%= link_to 'Edit', edit_empathy_path(empathy) %></td>
        <td><%= link_to 'Destroy', empathy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empathy', new_empathy_path %>
